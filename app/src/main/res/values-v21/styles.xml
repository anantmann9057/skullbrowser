<resources>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
       <item name="android:statusBarColor">#34499b</item>

    </style>

    <style name="mytheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimaryDark</item>
        <item name="android:statusBarColor">@android:color/background_dark</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@android:color/transparent</item>
        <item name="colorPrimaryDark">@android:color/holo_blue_light</item>
        <item name="colorAccent">@android:color/holo_red_dark</item>
        <item name="android:statusBarColor">?attr/actionModeSplitBackground</item>
        <item name="android:textColorPrimary">@color/text_color_primary</item>
    </style>

</resources>
